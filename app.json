{
  "expo": {
    "name": "Event",
    "slug": "event-discovery",
    "version": "1.0.4",
    "orientation": "portrait",
    "icon": "assets/icon_assets/Expo/icon.png",
    "userInterfaceStyle": "light",
    "statusBar": {
      "style": "light",
      "backgroundColor": "#000000",
      "translucent": true
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eventdiscovery.app",
      "buildNumber": "5",
      "icon": "assets/icon_assets/iOS_AppIcon.appiconset/Icon-App-1024x1024@1x.png",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses location to show events near you",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses location to show events near you",
        "NSCameraUsageDescription": "This app uses camera to take photos for events",
        "NSPhotoLibraryUsageDescription": "This app uses photo library to select images for events",
        "ITSAppUsesNonExemptEncryption": false,
        "UIStatusBarHidden": true,

      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "assets/icon_assets/Expo/icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "icon": "assets/icon_assets/Expo/icon.png",
      "package": "com.eventdiscovery.app",
      "versionCode": 4,
      "permissions": [
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "plugins": [
      "expo-location",
      "expo-camera",
      "expo-image-picker"
    ],
    "extra": {
      "eas": {
        "projectId": "e2541317-1823-49bd-9e1a-734fdc5511e1"
      }
    }
  }
}
